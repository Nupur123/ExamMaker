<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Name="ErrorLogWindow" x:Class="ExamMaker.ErrorLog"
        Title="ErrorLog"  Height="700" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Image HorizontalAlignment="Left" Height="150" Margin="10,10,0,0" VerticalAlignment="Top" Width="772" Source="Pictures/RobertsonBanner.jpg"/>
        <ScrollViewer CanContentScroll="True" Margin="55,200,57,104">
            <StackPanel>
                <TextBlock Name="txtLog" FontSize="16" TextWrapping="Wrap">No error log.</TextBlock>
            </StackPanel>
        </ScrollViewer>
        <Button x:Name="btnClose" Content="Close" HorizontalAlignment="Left" Margin="330,590,0,0" VerticalAlignment="Top" Width="75" Click="btnClose_Click"/>
    </Grid>
</Window>
